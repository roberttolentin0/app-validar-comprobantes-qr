class Comprobante():
    def __init__(
            self,
            id,
            ruc,
            fecha_emision,
            serie,
            numero,
            monto,
            id_tipo_comprobante) -> None:
        self.id=id
        self.ruc=ruc
        self.fecha_emision=fecha_emision
        self.serie=serie
        self.numero=numero
        self.monto=monto
        self.id_tipo_comprobante=id_tipo_comprobante

    def to_json(self):
        return {
            'id': self.id,
            'ruc': self.ruc,
            'fecha_emision': self.fecha_emision,
            'serie': self.serie,
            'numero': self.numero,
            'monto': self.monto,
            'id_tipo_comprobante': self.id_tipo_comprobante
        }